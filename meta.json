{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "viam:beagleboard",
  "visibility": "public",
  "url": "https://github.com/viam-modules/beagleboard",
  "description": "Go module for beagleboard beaglebone board, compatible with Viam",
  "models": [
    {
      "api": "rdk:component:board",
      "model": "viam:beagleboard:beaglebone",
      "markdown_link": "README.md#configure-your-beaglebone-board",
      "short_description": "board model for the BeagleBone AI 64"
    },
    {
      "api": "rdk:component:board",
      "model": "viam:beagleboard:beagle-y-ai",
      "markdown_link": "README.md#configure-your-beagley-ai-board",
      "short_description": "board model for the BeagleY-AI"
    }
  ],
  "build": {
    "build": "make module",
    "path": "bin/module.tar.gz",
    "arch": [
      "linux/arm64"
    ]
  },
  "entrypoint": "bin/beagleboard"
}
